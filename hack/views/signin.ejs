 <div ng-controller="signinCtrl" class="signin">
 {{result}}
 <form ng-submit="signInForm(userData)">
  	<table class="signinformtable">
 		<tr>
 			<td><input type="radio" name="sigininType" value="buyer" ng-model="usrType"/>Want To Buy</td>
 			<td><input type="radio" name="sigininType" value="seller" ng-model="usrType"/>Want To Sell</td>
 		</tr>
 		<tr>
 			<td class="txtalignrt"> UserName : </td>
 			<td><input type="text" ng-model="userData.uName"/></td>
 		</tr>
 		<tr>
 			<td class="txtalignrt"> Password : </td>
 			<td><input type="password" ng-model="userData.uPwd"/></td>
 		</tr>
 		<tr>
 			<td> </td>
 			<td><input type="submit" value="SignIn" class="signinbtn"/></td>
 		</tr>
 	</table>
  </form>
 </div>